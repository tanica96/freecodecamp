<tr class="product-search__product-row" data-fdc="<%= fdcId %>">
  <td>
    <span><%= title %></span>
  </td>
  <td>
    <input class="product-amount" type="number" value="100" min="0" />
    <button 
      type="button" 
      class="close product-search__remove-product" 
      data-dismiss="alert" 
      aria-label="Close"
      data-fdc="<%= fdcId %>">
      <span aria-hidden="true">&times;</span>
    </button>
  </td>
</tr>